/* pages/logistics/logistics.wxss */
.clearfix:after{
  display:block;
  clear:both;
  content:"";
  visibility:hidden;
  height:0
  } 
.clearfix{
  zoom:1
  } 
.logistics-wrapper{
  background:#fff;
  width:100%;
  height:100%;
}
.logistics-header{
  height:200rpx;
  padding:0 24rpx;
  border-top:1rpx solid #e7e7e7;
  line-height:56rpx;
  padding-top:20rpx;
}
.background{
  width:100%;
  height:20rpx;
  background:#e7e7e7;
}
.order-tracking{
  color:#9B9B9B;
  border-bottom:1rpx solid #e7e7e7;
  margin-bottom:20rpx;
  width:88%;
  float:right;
}
.logistics-circle{
  width:20rpx;
  height:20rpx;
  border-radius: 100%;
  background:#9B9B9B;
  position:absolute;
  left:24rpx;
  top:34rpx;
}
.logisticsbox{
  position:relative;
}
.logistics-content{
  margin-top:20rpx;
  background:#fff;
}
.logistics-none{
  text-align: center;
}